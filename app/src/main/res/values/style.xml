<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="Signup_Form_LL">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:padding">9dp</item>
    </style>

    <style name="Signup_Form_ET">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:inputType">text</item>
        <item name="android:padding">0dp</item>
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">#989898</item>
        <item name="android:textColorHint">#bababa</item>
        <item name="android:background">#00000000</item>
    </style>

    <style name="Signup_Form_IV">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">10dp</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="Account_Btn">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:background">#fff</item>
        <item name="android:textColor">#5c5c5c</item>
        <item name="android:layout_marginBottom">20dp</item>
    </style>

</resources>